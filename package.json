{
  "private": true,
  "name": "spryker-master-suite",
  "description": "Spryker Master Suite",
  "author": {
    "name": "Spryker Systems GmbH",
    "email": "info@spryker.com",
    "url": "https://spryker.com/"
  },
  "scripts": {
    "yves": "node ./frontend/build development",
    "yves:watch": "node ./frontend/build development-watch",
    "yves:production": "node ./frontend/build production",
    "yves:help": "node ./frontend/libs/command-line-parser --help",
    "yves:stylelint": "NODE_ENV=development node ./frontend/libs/stylelint development",
    "yves:tslint": "node ./frontend/libs/tslint",
    "zed": "node ./node_modules/@spryker/oryx-for-zed/build",
    "zed:watch": "node ./node_modules/@spryker/oryx-for-zed/build --dev",
    "zed:production": "node ./node_modules/@spryker/oryx-for-zed/build --prod",
    "bo:install": "run-s bo:install:*",
    "bo:install:zed-ui": "npm run cd:zed-ui -- npm install",
    "bo:install:mp-login": "npm run cd:mp-login -- npm install",
    "bo:install:mp-dashboard": "npm run cd:mp-dashboard -- npm install",
    "bo:install:mp-profile": "npm run cd:mp-profile -- npm install",
    "bo:install:mp-product-offer": "npm run cd:mp-product-offer -- npm install",
    "bo:build": "npm run fe:root -- run-s bo:build:*",
    "bo:build:zed-ui": "npm run cd:zed-ui -- npm run build",
    "bo:build:mp-login": "npm run cd:mp-login -- npm run build",
    "bo:build:mp-dashboard": "npm run cd:mp-dashboard -- npm run build",
    "bo:build:mp-profile": "npm run cd:mp-profile -- npm run build",
    "bo:clean": "run-s bo:clean:*",
    "bo:clean:dist": "rimraf vendor/spryker/*/dist",
    "bo:clean:modules": "rimraf vendor/spryker/*/node_modules",
    "bo:test": "run-s bo:test:*",
    "bo:test:zed-ui": "npm run cd:zed-ui -- npm run test -- --watch=false --browsers ChromeHeadless",
    "bo:test:mp-login": "npm run cd:mp-login -- npm run test -- --watch=false --browsers ChromeHeadless",
    "bo:test:mp-dashboard": "npm run cd:mp-dashboard -- npm run test -- --watch=false --browsers ChromeHeadless",
    "cd:zed-ui": "cd ./vendor/spryker/zed-ui && ",
    "cd:mp-login": "cd ./vendor/spryker/authentication-merchant-portal-gui && ",
    "cd:mp-dashboard": "cd ./vendor/spryker/dashboard-merchant-portal-gui && ",
     "cd:mp-profile": "cd ./vendor/spryker/merchant-profile-merchant-portal-gui && ",
    "fe:root": "export FE_ROOT_DIR=\"../../..\" && "
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": ">=6.4.1"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "~1.3.1",
    "@webcomponents/webcomponents-platform": "^1.0.1",
    "@webcomponents/webcomponentsjs": "~2.4.0",
    "classlist-polyfill": "~1.2.0",
    "core-js": "~3.4.1",
    "date-input-polyfill": "~2.14.0",
    "element-closest": "~3.0.0",
    "lodash-es": "~4.17.15",
    "string.prototype.startswith": "~0.2.0"
  },
  "devDependencies": {
    "@spryker/oryx-for-zed": "^2.5.3",
    "@spryker/sass-resources-loader": "github:spryker/sass-resources-loader",
    "autoprefixer": "~8.6.2",
    "clean-webpack-plugin": "~1.0.1",
    "commander": "~4.0.1",
    "copy-webpack-plugin": "~5.0.5",
    "css-loader": "~3.2.0",
    "fast-glob": "~3.1.0",
    "mini-css-extract-plugin": "~0.8.0",
    "node-sass": "~4.13.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "~5.0.1",
    "postcss-loader": "~3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "~8.0.0",
    "stylelint": "~12.0.0",
    "ts-loader": "~6.2.1",
    "tslint": "~5.20.1",
    "tslint-lines-between-class-members": "~1.3.6",
    "typescript": "~3.7.2",
    "typescript-eslint-parser": "~22.0.0",
    "uglifyjs-webpack-plugin": "~1.3.0",
    "webpack": "~4.41.2",
    "webpack-merge": "~4.2.2"
  }
}
