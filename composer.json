        "spryker/development-core": "dev-master",
        "spryker/code-generator": "dev-master",
        "spryker/code-sniffer": "dev-master",
        "spryker/git-hook": "dev-master",
        "pdepend/pdepend": "dev-master as 2.11.0",
        "phpmd/phpmd": "^2.0.0",
        "composer/package-versions-deprecated": "^1.8.0",
        "symfony/web-profiler-bundle": "^4.0.0 || ^5.0.0",
        "symfony/var-dumper": "*",
        "spryker-sdk/codeception-phantoman": "dev-master",
        "stecman/symfony-console-completion": "*",
        "fakerphp/faker": "^1.16.0",
        "filp/whoops": "^2.7",
        "spryker-sdk/phpstan-spryker": "^0.4.0",
        "spryker-sdk/composer-constrainer": "dev-master",
        "spryker-sdk/composer-replace": "dev-master",
        "spryker-sdk/spryk": "dev-master as 0.4.x-dev",
