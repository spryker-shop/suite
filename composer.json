        "codeception/module-phpbrowser": "^3.0.0",
        "codeception/module-asserts": "^3.0.0",
        "codeception/module-webdriver": "^4.0.1",
        "codeception/module-cli": "^2.0.0",
        "codeception/module-filesystem": "^3.0.0",
        "codeception/stub": "^3.7.0 || ^4.0",
        "sebastian/diff": "*",
        "phpstan/phpstan": "1.12.29",
        "symfony/browser-kit": "~5.1.8  || ^5.4",
        "nette/neon": "^3.2.1",
        "spryker-sdk/benchmark": "dev-master",
        "spryker/docker-chromedriver": "dev-master",
        "spryker-sdk/load-testing": "dev-master",
        "codeception/c3": "^2.6",
        "spryker-sdk/evaluator": "dev-master",
        "spryker-sdk/async-api": "^0.3.1",
        "spryker/profiler": "dev-master",
        "spryker/commerce-k6-performance-tests": "dev-master",
        "spryker/cypress-tests": "dev-master",
        "phpstan/phpdoc-parser": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "Generated\\": "src/Generated/",
            "Orm\\": "src/Orm/",
            "Pyz\\": "src/Pyz/",
            "SprykerConfig\\": "src/SprykerConfig/",
            "Propel\\": "vendor/spryker/spryker/Bundles/PropelOrm/src/Spryker/Zed/PropelOrm/Business/Polyfill/"
        },
        "psr-0": {
            "Pimple": "vendor/spryker/spryker/Bundles/Container/src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyzTest\\": "tests/PyzTest/",
            "VolumeDataGenerationTest\\": "tests/VolumeDataGenerationTest/",
            "SprykerFeatureTest\\": "tests/PyzFeatureTest/",
            "Benchmark\\": "tests/Benchmark/",
            "Symfony\\Contracts\\Translation\\": "vendor/symfony/translation-contracts/"
        },
        "files": [
            "test-autoload.php"
        ],
        "exclude-from-classmap": [
            "vendor/flow/jsonpath/tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.3.13"
        },
        "preferred-install": {
            "spryker/*": "source",
            "spryker-shop/*": "source",
            "spryker-eco/*": "source",
            "spryker-middleware/*": "source",
            "spryker-sdk/*": "source",
            "*": "dist"
        },
        "use-include-path": true,
        "process-timeout": 1800,
        "chromium-revision": 814168,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "codeception/c3": true,
            "spryker/composer-merge-plugin": true,
            "sllh/composer-versions-check": true,
            "symfony/runtime": false,
            "symfony/flex": false,
            "php-http/discovery": true,
            "tbachert/spi": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:spryker/spryker.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/spryker-shop.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/development-core.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/robotframework-suite-tests.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/commerce-k6-performance-tests.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/cypress-tests.git"
        }
    ],
    "extra": {
        "installer-types": [
            "spryker-test"
        ],
        "installer-paths": {
            "tests/cypress-tests/": [
                "spryker/cypress-tests"
            ]
        }
    },
    "scripts": {
        "compile": [
            "mkdir -p ./data/DE/logs/ZED"
        ],
        "post-install-cmd": [
            "ChromedriverInstaller\\Installer::installChromedriver"
        ],
        "post-update-cmd": [
            "ChromedriverInstaller\\Installer::installChromedriver"
        ],
        "phpstan": [
            "php -d memory_limit=-1 vendor/bin/phpstan"
        ],
        "phpstan-setup": "vendor/bin/console dev:ide-auto-completion:generate",
        "rm-githooks": [
            "echo 'Removing GitHooks in the project' && rm -f .git/hooks/pre-commit",
            "echo 'Removing GitHooks in spryker/spryker' && rm -f vendor/spryker/spryker/.git/hooks/pre-commit",
            "echo 'Removing GitHooks in spryker/spryker-shop' && rm -f vendor/spryker/spryker-shop/.git/hooks/pre-commit"
        ]
    },
    "conflict": {
        "codeception/codeception": "5.0.13"
    }
}
