        "codeception/phpunit-wrapper": "^9.0.0",
        "codeception/module-rest": "^1.0",
        "codeception/module-phpbrowser": "^1.0.1",
        "codeception/module-asserts": "^1.3.0",
        "codeception/module-webdriver": "^1.1.0",
        "codeception/module-cli": "^1.0.2",
        "codeception/module-filesystem": "^1.0.2",
        "codeception/stub": "^3.7.0",
        "psalm/phar": "^4.3.1",
        "sebastian/diff": "*",
        "phpstan/phpstan": "1.6.7",
        "symfony/browser-kit": "^4.0.0 || ~5.1.8",
        "nette/neon": "^3.2.1",
        "spryker-sdk/benchmark": "dev-master",
        "spryker/docker-chromedriver": "dev-master",
        "spryker-sdk/load-testing": "dev-master",
        "spryker-sdk/security-checker": "dev-master",
        "codeception/c3": "^2.6",
        "spryker-sdk/async-api": "^0.2.3"
    },
    "autoload": {
        "psr-4": {
            "Generated\\": "src/Generated/",
            "Orm\\": "src/Orm/",
            "Pyz\\": "src/Pyz/",
            "SprykerConfig\\": "src/SprykerConfig/"
        },
        "psr-0": {
            "Pimple": "vendor/spryker/spryker/Bundles/Container/src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyzTest\\": "tests/PyzTest/",
            "Benchmark\\": "tests/Benchmark/"
        },
        "files": [
            "test-autoload.php"
        ],
        "exclude-from-classmap": [
            "vendor/flow/jsonpath/tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4.15"
        },
        "preferred-install": {
            "spryker/*": "source",
            "spryker-shop/*": "source",
            "spryker-eco/*": "source",
            "spryker-middleware/*": "source",
            "spryker-sdk/*": "source",
            "*": "dist"
        },
        "use-include-path": true,
        "process-timeout": 1800,
        "chromium-revision": 814168,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/package-versions-deprecated": true,
            "codeception/c3": true,
            "spryker/composer-merge-plugin": true
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:spryker/spryker.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/spryker-shop.git"
        },
        {
            "type": "git",
            "url": "git@github.com:spryker/development-core.git"
        }
    ],
    "scripts": {
        "compile": [
            "mkdir -p ./data/DE/logs/ZED"
        ],
        "post-install-cmd": [
            "ChromedriverInstaller\\Installer::installChromedriver",
            "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
            "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
            "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
        ],
        "post-update-cmd": [
            "ChromedriverInstaller\\Installer::installChromedriver",
            "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerHooks",
            "GitHook\\Composer\\Scripts\\HookInstaller::installSprykerShopHooks",
            "GitHook\\Composer\\Scripts\\HookInstaller::installProjectHooks"
        ],
        "phpstan": [
            "php -d memory_limit=-1 vendor/bin/phpstan"
        ],
        "phpstan-setup": "vendor/bin/console dev:ide-auto-completion:generate",
        "psalm": "vendor/bin/psalm.phar --show-info=false --no-cache",
        "psalm-update-baseline": "vendor/bin/psalm.phar --update-baseline",
        "psalm-set-baseline": "vendor/bin/psalm.phar --set-baseline=psalm-baseline.xml",
        "psalm-update-report": "vendor/bin/psalm.phar --report=psalm-report.xml",
        "prefer-mid": [
            "COMPOSER_MEMORY_LIMIT=-1 composer require twig/twig:\"^2.9.0\""
        ]
    }
}
