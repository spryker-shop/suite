{# @var order \Generated\Shared\Transfer\OrderTransfer #}
{# @var invoice \Generated\Shared\Transfer\OrderInvoiceTransfer #}

<style type="text/css">
    body {margin-top: 0;margin-left: 0; font: 16px 'spryker';}
    #page_1 {position:relative; margin: 18px 0 70px 30px;padding: 0 0 60px 0;border: none;width:100%;}
    #page_1 #id1_1 {border:none;margin: 0 0 0 0;padding: 0;}
    .spacing-y-offset{margin: 15px 0;}
    .spacing-left{padding: 0 0 0 15px;}
    .spacing-bottom{padding: 0 0 15px;}
    .top{vertical-align: top;}
    .logo{width: 270px;margin:0 0 15px;}
    .font-bold{font-weight: bold;}
    .table-products TBODY TR:last-child TD{padding: 0 100px}
    .th0{margin: 0;width: 9%;vertical-align: middle;text-align: center;}
    .th1{margin: 0;width: 12%;vertical-align: middle;text-align: center;}
    .th2{margin: 0;width: 32%;vertical-align: middle;text-align: left;}
    .th3{margin: 0;width: 17%;vertical-align: middle;text-align: center;}
    .th4{margin: 0;width: 17%;vertical-align: middle;text-align: center;}
    .th5{margin: 0;width: 13%;vertical-align: middle;text-align: center;}
    .tr13{vertical-align: top;}
    .tr-gray{background: #e6e6e6;}
    .right{text-align: right;}
    .font-small{font-size: 13px;}
    @page {odd-footer-name: footer;even-footer-name: footer;margin-bottom: 25mm;}
</style>

<table cellpadding=0 cellspacing=0 class="t0">
    <tr>
        <td class="td00">
            <img class="logo" src="data:image/png;base64," alt="Logo">
        </td>
        <td class="td01 spacing-left top">
            <div>
                <div class="font-bold">{{ 'order_invoice.invoice_template.' | trans }}</div>
                <div class="font-small">
                    {{ 'order_invoice.invoice_template.' | trans }}<br/>
                </div>
                <br />
                <div class="font-small">
                    {{ 'order_invoice.invoice_template.' | trans }}<br/>
                    {{ 'order_invoice.invoice_template.' | trans }} {{ 'order_invoice.invoice_template.' | trans }}
                </div>
            </div>
        </td>
    </tr>
    <tr>
        <td class="spacing-bottom">
            <div class="font-bold">{{ 'order_invoice.invoice_template.' | trans }}</div>
            <div class="font-small">{{ 'order_invoice.invoice_template.' | trans }} Â· {{ 'order_invoice.invoice_template.' | trans }} {{ 'order_invoice.invoice_template.' | trans }}</div>
        </td>
        <td class="spacing-left top">{{ 'order_invoice.invoice_template.' | trans }}, {{ invoice.issueDate | date('d. m y') }}</td>
    </tr>
    <tr>
        <td colspan="2">
            {{ order.billingAddress.firstName }} {{ order.billingAddress.lastName }}<br />
            {{ order.billingAddress.address1 }} {{ order.billingAddress.address2 }} {{ order.billingAddress.address3 }}<br />
            {{ order.billingAddress.zipcode }} {{ order.billingAddress.city }}<br />
            {{ order.billingAddress.region }}
        </td>
    </tr>
</table>

<div class="spacing-y-offset font-bold">{{ 'order_invoice.invoice_template.reference'| trans }}: {{ invoice.reference }}</div>
<div class="spacing-y-offset">{{ 'order_invoice.invoice_template.goods' | trans }}:</div>

<table cellpadding="6px" autosize="2.4" class="t0 table-products" style="width:100%;border: 1px solid black;border-collapse: collapse;">
    <thead>
    <tr>
        <td class="tr-gray th0"><p class="p3 ft4 font-bold">{{ 'order_invoice.invoice_template.table.number'| trans }}.</p></td>
        <td class="tr-gray th1"><p class="p4 ft4 font-bold">{{ 'order_invoice.invoice_template.table.quantity' | trans }}</p></td>
        <td class="tr-gray th2"><p class="font-bold">{{ 'order_invoice.invoice_template.table.name' | trans }}</p></td>
        <td class="tr-gray th3"><p class="p5 ft4 font-bold">{{ 'order_invoice.invoice_template.table.tax' | trans }}</p></td>
        <td class="tr-gray th4"><p class="p5 ft4 font-bold">{{ 'order_invoice.invoice_template.table.price' | trans | raw }}</p></td>
        <td class="tr-gray th5"><p class="p5 ft4 font-bold">{{ 'order_invoice.invoice_template.table.sum' | trans | raw }}</p></td>
    </tr>
    </thead>
    <tfoot>
    <tr>
        <td colspan="6" style="padding: 0 0 0 0;"></td>
    </tr>
    </tfoot>
    <tbody>
    {% set linenumber = 0 %}
    {% for item in order.items %}
        {# @var item \Generated\Shared\Transfer\ItemTransfer #}
        {% if item.productBundle is not defined or item.productBundle is null %}
            {% set linenumber = linenumber + 1 %}
            <tr>
                <td class="tr13 right"><p class="p7 ft4">{{ linenumber }}</p></td>
                <td class="tr13 right"><p class="p4 ft4">{{ item.quantity }}</p></td>
                <td class="tr13"><p class="p1 ft4">{{ item.name }}</p></td>
                <td class="tr13 right"><p class="p6 ft4">{{ item.taxRate|number_format }}%</p></td>
                <td class="tr13 right"><p class="p6 ft4">{{ (item.sumPrice - item.sumDiscountAmountFullAggregation) | money(true, order.currencyIsoCode) }}</p></td>
                <td class="tr13 right"><p class="p6 ft4">{{ item.sumPriceToPayAggregation | money(true, order.currencyIsoCode) }}</p></td>
            </tr>
        {% endif %}
    {% endfor %}
    {% set renderedBundles = [] %}
    {% for item in order.items %}
        {# @var item \Generated\Shared\Transfer\ItemTransfer #}
        {% if item.productBundle is defined and item.productBundle is not null %}
            {% if item.relatedBundleItemIdentifier not in renderedBundles %}
                {% set linenumber = linenumber + 1 %}
                {# @var productBundle \Generated\Shared\Transfer\ItemTransfer #}
                {% set productBundle = item.productBundle %}
                <tr>
                    <td class="tr13 right"><p class="p7 ft4">{{ linenumber }}</p></td>
                    <td class="tr13 right"><p class="p4 ft4">{{ productBundle.quantity }}</p></td>
                    <td class="tr13"><p class="p1 ft4">{{ productBundle.name }}</p></td>
                    <td class="tr13 right"><p class="p6 ft4">{{ item.taxRate|number_format }}%</p></td>
                    <td class="tr13 right"><p class="p6 ft4">{{ (productBundle.sumPrice - productBundle.sumDiscountAmountFullAggregation) | money(true, order.currencyIsoCode) }}</p></td>
                    <td class="tr13 right"><p class="p6 ft4">{{ productBundle.sumPriceToPayAggregation | money(true, order.currencyIsoCode) }}</p></td>
                </tr>
                {% set renderedBundles = renderedBundles | merge([item.relatedBundleItemIdentifier]) %}
            {% endif %}
        {% endif %}
    {% endfor %}
    {% if order.expenses %}
        {% for expense in order.expenses %}
            {% set linenumber = linenumber + 1 %}
            <tr>
                <td class="tr13 right"><p class="p1 ft1">{{ linenumber }}</p></td>
                <td class="tr13 right"><p class="p6 ft4"></p></td>
                <td class="tr13"><p class="p5 ft4">{{ expense.name }}</p></td>
                <td class="tr13 right"><p class="p1 ft1">{{ expense.taxRate|number_format }}%</p></td>
                <td class="tr13 right"><p class="p1 ft1">{{ expense.sumPrice | money(true, order.currencyIsoCode) }}</p></td>
                <td class="tr13 right"><p class="p1 ft1">{{ expense.sumPrice | money(true, order.currencyIsoCode) }}</p></td>
            </tr>
        {% endfor %}
    {% endif %}
    <tr class="tr-gray">
        <td colspan="5" class="right"><p class="p4 ft4">{{ 'order_invoice.invoice_template.table.total.gross' | trans }}</p></td>
        <td class="right"><p class="p5 ft4">{{ order.totals.subtotal | money(true, order.currencyIsoCode) }}</p></td>
    </tr>
    <tr class="tr-gray">
        <td colspan="5" class="right"><p class="p4 ft4">{{ 'mail.invoice.table.total.gross' | trans }}</p></td>
        <td class="right"><p class="p5 ft4">{{ order.totals.discountTotal | money(true, order.currencyIsoCode) }}</p></td>
    </tr>

    {% set taxes = {} %}
    {% for item in order.items %}
        {# @var item \Generated\Shared\Transfer\ItemTransfer #}
        {% set rateSum = taxes[item.taxRate] | default(0) + item.sumTaxAmountFullAggregation %}
        {% set taxes = taxes | merge({(item.taxRate): rateSum}) %}
    {% endfor %}
    {% for rate, tax in taxes %}
        {% if tax %}
            <tr>
                <td colspan="3" class="tr13 td15 right"><p class="p5 ft4">{{ 'mail.invoice.table.vat.included' | trans({'%tax%': rate / 100 }) }}</p></td>
                <td class="tr13 right"><p class="p6 ft4">{{ tax | money(true, order.currencyIsoCode) }}</p></td>
                <td class="tr13 right"><p class="p5 ft4">{{ 'mail.invoice.table.vat' | trans }}</p></td>
                <td class="tr13 right"><p class="p6 ft4">{{ tax | money(true, order.currencyIsoCode) }}</p></td>
            </tr>
        {% endif %}
    {% endfor %}
    <tr>
        <td colspan="3" class="tr13 td15 right"><p class="p1 ft1">{{ 'mail.invoice.table.total.net' | trans }}</p></td>
        <td class="tr13 right"><p class="p6 ft4">{{ order.totals.taxTotal.amount | money(true, order.currencyIsoCode) }}</p></td>
    </tr>
    <tr class="tr-gray">
        <td colspan="5" class="right"><p class="p4 ft4 pb font-bold" style="text-align: right; ">{{ 'mail.invoice.table.payment.amount' | trans }}</p></td>
        <td class="right"><p class="p5 ft4 pb">{{ order.totals.grandTotal | money(true, order.currencyIsoCode) }}</p></td>
    </tr>
    </tbody>
</table>


